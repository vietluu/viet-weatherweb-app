{
    "name": "react-project",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://vietluu.github.io/viet-weatherweb-app.git/",
    "dependencies": {
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@testing-library/user-event": "^12.8.3",
        "axios": "^0.21.1",
        "font-awesome": "^4.7.0",
        "gh-pages": "^3.2.3",
        "history": "^4.10.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "react-slideshow-image": "^3.5.0",
        "web-vitals": "^1.1.1"
    },
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "react/jsx-uses-react": "error",
            "react/jsx-uses-vars": "error",
            "no-unused-vars": 0,
            "no-console": [
                "warn",
                {
                    "allow": [
                        "clear",
                        "info",
                        "error",
                        "dir",
                        "trace",
                        "log"
                    ]
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
